      <script>
        document.addEventListener('DOMContentLoaded', function() {
          const contactForm = document.getElementById('contact-form');
          const formInputs = document.querySelectorAll('.contact__form-input');
          
          // Add focus effect to form inputs
          formInputs.forEach(input => {
            input.addEventListener('focus', () => {
              input.parentElement.querySelector('.contact__form-label').style.color = 'var(--first-color)';
              input.parentElement.querySelector('.contact__form-label').style.transform = 'translateY(-3px)';
            });
            
            input.addEventListener('blur', () => {
              input.parentElement.querySelector('.contact__form-label').style.color = 'var(--text-color)';
              input.parentElement.querySelector('.contact__form-label').style.transform = 'translateY(0)';
            });
          });
          
          // Handle form submission
          contactForm.addEventListener('submit', function(e) {
            e.preventDefault();
            
            // Simulate form submission with animation
            const button = contactForm.querySelector('.contact__form-button');
            button.disabled = true;
            button.innerHTML = 'Sending...';
            
            setTimeout(() => {
              button.innerHTML = 'Message Sent!';
              button.style.background = '#4caf50';
              
              // Reset form
              setTimeout(() => {
                contactForm.reset();
                button.innerHTML = 'Send Message <div class="contact__form-button-icon-container"><i class="uil uil-message button__icon contact__form-button-icon"></i></div>';
                button.disabled = false;
                button.style.background = '';
              }, 2000);
            }, 1500);
          });
        });
      </script>

